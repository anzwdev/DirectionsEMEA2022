<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="github" value="https://nuget.pkg.github.com/<MyGitHubWorkspace>/index.json" />
        <add key="azuredevops" value="https://pkgs.dev.azure.com/<MyAzureWorkspace>/<MyAzureRepository>/_packaging/<MyAzureRepository>/nuget/v3/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <github>
            <add key="Username" value="<MyGitHubUserName>" />
            <add key="ClearTextPassword" value="<MyGitHubApiKey>" />
        </github>
    </packageSourceCredentials>
</configuration>
